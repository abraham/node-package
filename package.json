{
  "name": "node-package",
  "version": "1.1.1",
  "description": "Node Package Web Component",
  "main": "dist/node-package.js",
  "module": "dist/node-package.js",
  "unpkg": "dist/node-package.min.js",
  "types": "dist/node-package.d.ts",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abraham/node-package.git"
  },
  "engines": {
